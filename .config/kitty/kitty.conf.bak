font_family Ligalex Mono SemiBold Nerd Font Mono
bold_font Ligalex Mono Bold Nerd Font Mono
italic_font Ligalex Mono SemiBoldItalic Nerd Font Mono
bold_italic_font Ligalex Mono BoldItalic Nerd Font Mono

/* font_family Fira Code SemiBold Nerd Font Complete */
/* bold_font Fira Code Bold Nerd Font Complete */
/* italic_font Fira Code SemiBold Nerd Font Complete */
/* bold_italic_font Fira Code Medium Nerd Font Complete */

/* font_family JetBrains Mono SemiBold Nerd Font Complete Mono */
/* bold_font JetBrains Mono Bold Nerd Font Complete Mono */
/* italic_font JetBrains Mono SemiBold Italic Nerd Font Complete Mono */
/* bold_italic_font JetBrains Mono Bold Italic Nerd Font Complete Mono */

shell_integration disabled

font_size 12
background_opacity 0.8

window_padding_width 10
confirm_os_window_close 0

macos_thicken_font 0.75

repaint_delay 5
sync_to_monitor no

# Upstream colors {{{
# foreground #a6accd
# background #0d0f16

# Black
# color0 #1b1e28
# color8 #a6accd

# Red
# color1 #d0679d
# color9 #d0679d

# Green
# color2  #5de4c7
# color10 #5de4c7

# Yellow
# color3  #fffac2
# color11 #fffac2

# Blue
# color4  #89ddff
# color12 #add7ff

# Magenta
# color5  #fcc5e9
# color13 #fae4fc

# Cyan
# color6  #add7ff
# color14 #89ddff

# White
# color7  #ffffff
# color15 #ffffff

# Cursor
# cursor #ffffff
# cursor_text_color #1b1e28

# Selection highlight
# selection_foreground none
# selection_background #28344a

# The color for highlighting URLs on mouse-over
# url_color #9ece6a
url color #5de4c7

# Window borders
# active_border_color #3d59a1
# inactive_border_color #101014
# bell_border_color #fffac2

# Tab bar
tab_bar_style fade
tab_fade 1
# active_tab_foreground   #3d59a1
# active_tab_background   #16161e
active_tab_font_style   bold
# inactive_tab_foreground #787c99
# inactive_tab_background #16161e
inactive_tab_font_style bold
# tab_bar_background #101014

# Title bar
# macos_titlebar_color #16161e
# }}}

# {{{ Keybindings
map kitty_mod+t     new_tab_with_cwd
# }}}
